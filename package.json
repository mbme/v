  "scripts": {
    "build": "",
    "start": "node bin/server.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "modules": false
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    },
    "plugins": [
      "transform-object-rest-spread",
      "syntax-jsx",
      "transform-react-jsx",
      "transform-react-display-name",
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "node": true,
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": [
      "airbnb",
      "standard"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "max-len": 0,
      "no-console": 0,
      "no-confusing-arrow": 0,
      "prefer-arrow-callback": 0,
      "prefer-template": 0,
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "react/require-default-props": 0,
      "react/forbid-prop-types": 0
    }
  }
}
